<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ncpcr.visionaries.sishutraan"
    >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="activity.AppController"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/MyMaterialTheme.Base" >
        <activity android:name="activity.Splash" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="activity.Home">
            <intent-filter>
                <action android:name="com.ncpcr.visionaries.sishutraan.HOME" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="activity.Home2">

        </activity>
    </application>

</manifest>
